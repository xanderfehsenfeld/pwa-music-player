{
  "name": "pwa-playlist",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.4.1",
    "@material-ui/icons": "^4.4.1",
    "@material-ui/lab": "^4.0.0-alpha.26",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.4",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "fast-average-color": "^5.2.0",
    "firebase": "^6.6.0",
    "firebase-functions": "^3.2.0",
    "firebase-tools": "^7.3.1",
    "history": "^4.9.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-loadable": "^5.5.0",
    "react-material-ui-form-validator": "^2.0.9",
    "react-scripts": "^3.1.1",
    "react-test-renderer": "^16.9.0",
    "typescript": "^3.6.2"
  },
  "engines": {
    "node": ">=10"
  },
  "reactSnap": {
    "inlineCss": true,
    "include": [
      "/beats",
      "/songs"
    ]
  },
  "config-overrides-path": "node_modules/react-app-rewire-disable-chunks/config-overrides.js",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --runInBand --no-cache",
    "coverage": "react-app-rewired test --coverage",
    "eject": "react-scripts eject",
    "postbuild": "react-snap"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "resetMocks": true,
    "collectCoverageFrom": [
      "**/src/app/index.js",
      "**/src/components/**/*.js",
      "**/src/pages/**/*.js",
      "**/src/helpers/*.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/history": "^4.7.3",
    "@types/lodash": "^4.14.138",
    "@types/react-material-ui-form-validator": "^2.0.3",
    "@types/webaudioapi": "0.0.27",
    "react-app-rewire-disable-chunks": "0.0.1",
    "react-app-rewired": "^2.1.3",
    "react-snap": "^1.23.0"
  }
}
